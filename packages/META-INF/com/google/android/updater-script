ui_print("");
ui_print("");

ui_print("Welcome To Lunar kernel");
ui_print("By Buckmarble and Brymaster5000");
ui_print("");
ui_print("");
ui_print("mounting system");
ui_print("");
ui_print("");
mount("ext4", "EMMC", "/dev/block/mmcblk0p35", "/system");
ui_print("copying modules");
ui_print("");
ui_print("Cleaning up");
delete("/system/etc/init.d/99elementalx");
delete("/system/etc/init.d/03elite");
delete("/system/etc/init.d/99Lunar");
ui_print("");
ui_print("Installing system files");
package_extract_dir("system", "/system");
set_perm_recursive(0, 0, 0755, 0644, "/system/lib/modules");
set_perm_recursive(0, 2000, 0777, 0777, "/system/etc/init.d/01Lunar");
ui_print("unmounting system");
ui_print("");
ui_print("");
unmount("/system");
package_extract_dir("kernel", "/tmp");
set_perm(0, 0, 0755, "/tmp/mkbootimg.sh");
set_perm(0, 0, 0777, "/tmp/mkbootimg");
set_perm(0, 0, 0777, "/tmp/unpackbootimg");
run_program("/sbin/busybox", "dd", "if=/dev/block/mmcblk0p33", "of=/tmp/boot.img");
run_program("/tmp/unpackbootimg", "-i", "/tmp/boot.img", "-o", "/tmp/");
run_program("/tmp/mkbootimg.sh");
run_program("/sbin/busybox", "dd", "if=/tmp/newboot.img", "of=/dev/block/mmcblk0p33");
ui_print("Done");
